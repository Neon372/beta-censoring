@page "/config"
@model MyApp.Namespace.ConfigurationModel
@{
    Layout = "Shared/_Layout.cshtml";
    ViewData["Title"] = "Server Configuration";
    ViewData["EntryPoint"] = "config";
}
@Html.AntiForgeryToken()
<div id="app"></div>

@section scripts {
    <environment names="Development">
        <script type="module" src="~/dist/config.ts"></script>
    </environment>
}